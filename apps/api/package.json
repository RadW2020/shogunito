    "@nestjs/schematics": "^11.0.0",
    "@swc/cli": "^0.7.9",
    "@swc/core": "^1.15.7",
    "@types/express": "^5.0.0",

    "@types/node": "^22.10.7",

    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",

    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
